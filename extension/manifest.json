{
	"manifest_version": 2,
	"name": "Roblox DevHub Search",
	"version": "1.1.3",
	"description": "Search the Roblox DevHub",
	"author": "Stephen Leitnick",
	"options_page": "options/options.html",
	"browser_action": {
		"default_popup": "popup/popup.html"
	},
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"permissions": ["storage"]
}