{
	"manifest_version": 3,
	"name": "Roblox DevHub Search",
	"version": "1.4.5",
	"description": "Search the Roblox DevHub",
	"author": "Stephen Leitnick",
	"options_ui": {	
		"page": "options/options.html",
		"browser_style": true,
		"open_in_tab": false
	},
	"action": {
		"default_popup": "popup/popup.html"
	},
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"96": "icons/96.png",
		"128": "icons/128.png"
	},
	"permissions": ["storage"]
}
